############################################################
# Example environment variables for eth-wallet-dashboard    #
# Copy this file to .env.development and .env.production   #
# and fill in your values                                  #
############################################################

# Error reporting
VITE_SENTRY_DSN=your-sentry-dsn-here

# Environment
MODE=development # or production

# Etherscan API Key
# Register and get your API key at https://etherscan.io/myapikey
# For development use your key or leave empty to use Wagmi fallback
VITE_ETHERSCAN_API_KEY="your-etherscan-api-key-here"

# Example for production:
# VITE_ETHERSCAN_API_KEY="#" # disables Etherscan, uses Wagmi only

############################################################
# Etherscan API Key is required for transaction history     #
# Register and get your API key at https://etherscan.io/myapikey
# For development use your key or leave empty to use Wagmi fallback
VITE_ETHERSCAN_API_KEY="your-etherscan-api-key-here"

# Example for production:
# VITE_ETHERSCAN_API_KEY="#" # disables Etherscan, uses Wagmi only
